{
  "version": "0.0.0",
  "keywords": [],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-admin": {
      "types": "dist/admin/src/index.d.ts",
      "source": "admin/src/index.ts",
      "import": "dist/admin/index.mjs",
      "require": "dist/admin/index.js",
      "default": "dist/admin/index.js"
    },
    "./strapi-server": {
      "types": "dist/server/src/index.d.ts",
      "source": "server/src/index.ts",
      "import": "dist/server/index.mjs",
      "require": "dist/server/index.js",
      "default": "dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "develop": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:front": "run -T tsc -p admin/tsconfig.json",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "@strapi/design-system": "^2.0.0-rc.16",
    "@strapi/icons": "^2.0.0-rc.16",
    "jwt-decode": "^4.0.0",
    "react-intl": "^7.1.6",
    "react-quill": "^2.0.0"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.1",
    "@strapi/strapi": "^5.10.2",
    "@strapi/typescript-utils": "^5.10.2",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "prettier": "^3.5.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.29.0",
    "styled-components": "^6.1.15",
    "typescript": "^5.7.3"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.1",
    "@strapi/strapi": "^5.10.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.29.0",
    "styled-components": "^6.1.15"
  },
  "strapi": {
    "kind": "plugin",
    "name": "submissions2",
    "displayName": "Submissions",
    "description": "POC for submissions and Approval amongst Editors and Contributers",
    "watch": {
      "admin": true,
      "server": true,
      "development": true
    }
  },
  "name": "submissions2",
  "description": "POC for submissions and Approval amongst Editors and Contributers",
